{
  "author": "Cory Reed",
  "description": "An experiment with icons for multiple platforms.",
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/react": "^17.0.16",
    "@types/react-native": "^0.64.12",
    "globby": "^12.0.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^10.2.0",
    "sharp": "^0.28.3",
    "svg2vectordrawable": "2.6.26",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "icons",
    "react",
    "react-native"
  ],
  "license": "MIT",
  "name": "multiplatformicon",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:android": "node scripts/build-android.mjs",
    "build:ios": "node scripts/build-ios.mjs",
    "build:react": "svgr -d react --typescript src && tsc -b tsconfig.react.json",
    "build:react-native": "node scripts/build-react-native.mjs && tsc -b tsconfig.react-native.json",
    "clean": "rm -rf android/src/main/res/drawable ios react react-native"
  },
  "version": "0.0.0"
}
